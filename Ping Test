!!! SKIP IF YOU CREATED INTERNAL, PING_SWEEP AND TARGETS FOLDERS !!!

(VICTIM) for i in {1..254}; do (ping -c 1 192.168.1.$i | grep "bytes from" &); done

copy and save

(ATTK) mkdir internal

cd internal

ls

nano ping_sweep

cat ping_sweep

cat ping_sweep | awk -F ":" '{print }'

cat ping_sweep | awk -F ":" '{print }'at ping_sweep | awk -F " " '{print }'

cat ping_sweep | awk -F ":" '{print }'at ping_sweep | awk -F " " '{print }' | sort -V

cat ping_sweep | awk -F ":" '{print }'at ping_sweep | awk -F " " '{print }' | sort -V >> targets

cat targets
